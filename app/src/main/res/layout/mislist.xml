<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:id="@+id/relativeLayoutMenu"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <RelativeLayout
        android:id="@+id/relativeLayoutreport"
        android:layout_width="wrap_content"
        android:minHeight="@dimen/space_96"
        android:background="@drawable/border_menu_list"
        android:layout_height="wrap_content"
        android:gravity="center">
        <androidx.cardview.widget.CardView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            card_view:cardUseCompatPadding="true"
            android:id="@+id/card_view"
            card_view:cardCornerRadius="2dp"
            android:shape="ring"
            android:layout_centerHorizontal="true"
            card_view:cardBackgroundColor="@color/transparent"
            android:elevation="@dimen/space_2"
            card_view:cardMaxElevation="2dp"
            card_view:cardElevation="@dimen/space_2"
            >

    <ImageView
        android:id="@+id/fileimage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:minHeight="@dimen/space_32"
        android:minWidth="@dimen/space_32"
        android:gravity="center"
        card_view:srcCompat="@drawable/ic_upload_black" />
        </androidx.cardview.widget.CardView>

    <TextView
        android:id="@+id/my"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/card_view"
        android:layout_marginBottom="9dp"
        android:gravity="center"
        android:text="@string/app_name"
        android:visibility="visible"
        android:textColor="@color/colorDark"
        android:textSize="@dimen/textLarge" />
    </RelativeLayout>

</RelativeLayout>